{
  "name": "moodify-backend",
  "version": "1.0.0",
  "description": "Storing data and breaking down images for their colour palettes!",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node server/server.js",
    "dev": "nodemon server/server.js",
    "build": "npm run dev",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:reset": "knex migrate:reset",
    "test": "cross-env NODE_ENV=test NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" jest",
    "test:watch": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:integration": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest --testPathPatterns=integration",
    "test:unit": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest --testPathPatterns=unit",
    "test:ci": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest --coverage --watchAll=false --passWithNoTests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OriginalByteMe/moodify-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/OriginalByteMe/moodify-backend/issues"
  },
  "homepage": "https://github.com/OriginalByteMe/moodify-backend#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "jpeg-js": "^0.4.4",
    "knex": "^3.1.0",
    "mongodb": "^6.16.0",
    "pg": "^8.16.0",
    "png-js": "^1.0.0"
  },
  "devDependencies": {
    "@testcontainers/postgresql": "^11.5.1",
    "cross-env": "^10.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "jest-mock-extended": "^4.0.0",
    "knex-mock-client": "^3.0.2",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4"
  }
}
